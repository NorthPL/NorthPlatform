    <generatorTypes>
        <generatorType>
            <name>island</name>
            <overload>64</overload>
            <items>
                <generatorItem material="IRON_INGOT" every="40" startAt="0"/>
                <generatorItem material="GOLD_INGOT" every="80"/>
            </items>
        </generatorType>

        <generatorType>
            <name>diamond</name>
            <overload>5</overload>
            <hudItem>DIAMOND_BLOCK</hudItem>
            <items>
                <generatorItem material="DIAMOND" name="I" every="600"/> <!-- start co 30 sekund -->
                <generatorItem material="DIAMOND" name="II" every="500" startAt="4200"/> <!-- Po 4 minutach co 25 sekund -->
                <generatorItem material="DIAMOND" name="III" every="400" startAt="9000"/> <!-- Po 9 minutach co 20 sekund -->
            </items>
        </generatorType>

        <generatorType>
            <name>emerald</name>
            <overload>4</overload>
            <hudItem>EMERALD_BLOCK</hudItem>
            <items>
                <generatorItem material="EMERALD" name="I" every="1200" startAt="0"/> <!-- start co 60 sekund -->
                <generatorItem material="EMERALD" name="II" every="1000" startAt="12000"/> <!-- po 5 minutach co 50 sekund -->
            </items>
        </generatorType>

        <generatorType>
            <name>lapis</name>
            <overload>64</overload>
            <hudItem>LAPIS_BLOCK</hudItem>
            <items>
                <generatorItem material="INK_SACK" data="4" name="I" every="6000" startAt="12000"/>
            </items>
        </generatorType>
    </generatorTypes>

