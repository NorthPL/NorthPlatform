<?xml version="1.0" encoding="UTF-8" ?>
<gui height="5" title="@main.window_title">
	<container pos="0,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
	<container pos="8,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
    <container pos="1,0" size="7,5">
        <content>

            <button pos="3,0">
            	<icon id="ARMOR_STAND" name="@main.technik.architekt.name" />
            </button>
            
            <button pos="2,2">
            	<icon id="WATCH" name="@technik.architekt.timefolder.name$color" lore="@technik.architekt.timefolder.lore$color,count,max"/>
            	<metadata key="guiName" value="technik_architekt_czas" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="engineer.architect.time:1" />
            	<metadata key="item2" value="engineer.architect.time:2" />
            	<metadata key="item3" value="engineer.architect.time:3" />
            	<metadata key="item4" value="engineer.architect.time:4" />
            	<metadata key="item5" value="engineer.architect.time:5" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="3,2">
            	<icon id="CHAINMAIL_CHESTPLATE" name="@technik.architekt.armor1.name$color" lore="@technik.architekt.armor1.lore$color,status,price"/>
            	<metadata key="item" value="engineer.architect.chainchest" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="4,2">
            	<icon id="WOOD" name="@technik.architekt.budulecfolder.name$color" lore="@technik.architekt.budulecfolder.lore$color,count,max" />
            	<metadata key="guiName" value="technik_architekt_budulec" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="engineer.architect.budulec:1" />
            	<metadata key="item2" value="engineer.architect.budulec:2" />
            	<metadata key="item3" value="engineer.architect.budulec:3" />
            	<metadata key="item4" value="engineer.architect.budulec:4" />
            	<metadata key="item5" value="engineer.architect.budulec:5" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="6,4">
            	<icon id="minecraft:iron_door" name="@main.return" />
            	<metadata key="guiName" value="technik" />
            	<onClick>open</onClick>
            </button>
        </content>
    </container>
</gui>