<?xml version="1.0" encoding="UTF-8" ?>
<gui height="5" title="@window_title">
	<container pos="0,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
	<container pos="8,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
    <container pos="1,0" size="7,5">
        <content>

            <button pos="3,0">
            	<icon id="minecraft:stick" name="@title" />
            </button>
            
            <button pos="1,2">
            	<icon id="minecraft:clock" name="@loadingtime.name$color" lore="@loadingtime.lore$color,count,max"/>
            	<metadata key="guiName" value="assasyn_czas" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="assasin.loadingtime:1" />
            	<metadata key="item2" value="assasin.loadingtime:2" />
            	<metadata key="item3" value="assasin.loadingtime:3" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="2,2">
            	<icon id="minecraft:iron_boots" name="@boots.name$color" lore="@boots.lore$color,status,price" />
            	<metadata key="item" value="assasin.boots" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="3,2">
            	<icon id="minecraft:leather_leggings" name="@leggings.name$color" lore="@leggings.lore$color,status,price" />
            	<metadata key="item" value="assasin.leggings" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="4,2">
            	<icon id="minecraft:iron_chestplate" name="@chestplate.name$color" lore="@chestplate.lore$color,status,price"/>
            	<metadata key="item" value="assasin.chestplate" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="5,2">
            	<icon id="minecraft:planks" name="@budulec.name$color" lore="@budulec.lore$color,count,max" />
            	<metadata key="guiName" value="assasyn_budulec" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="assasin.budulec:1" />
            	<metadata key="item2" value="assasin.budulec:2" />
            	<metadata key="item3" value="assasin.budulec:3" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="6,4">
            	<icon id="minecraft:iron_door" name="@return" />
            	<metadata key="guiName" value="main" />
            	<onClick>open</onClick>
            </button>
        </content>
    </container>
</gui>