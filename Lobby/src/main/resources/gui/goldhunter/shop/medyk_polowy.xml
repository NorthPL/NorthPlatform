<?xml version="1.0" encoding="UTF-8" ?>
<gui height="5" title="@main.window_title">
	<container pos="0,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
	<container pos="8,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
    <container pos="1,0" size="7,5">
        <content>

            <button pos="3,0">
            	<icon id="minecraft:slime_ball" name="@main.medyk.polowy.name" />
            </button>
            
            <button pos="1,2">
            	<icon id="minecraft:slime_ball" name="@buy.medyk.polowy.name$color" lore="@buy.medyk.polowy.lore$color,count,max"/>
            	<metadata key="item" value="class.medic.healer" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="3,2">
            	<icon id="WATCH" name="@medyk.polowy.timefolder.name$color" lore="@medyk.polowy.timefolder.lore$color,count,max"/>
            	<metadata key="guiName" value="medyk_polowy_czas" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="medic.healer.time:1" />
            	<metadata key="item2" value="medic.healer.time:2" />
            	<metadata key="item3" value="medic.healer.time:3" />
            	<metadata key="item4" value="medic.healer.time:4" />
            	<metadata key="item5" value="medic.healer.time:5" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="4,2">
            	<icon id="minecraft:potion" name="@medyk.polowy.potionfolder.name$color" lore="@medyk.polowy.potionfolder.lore$color,count,max">
            		<potionMeta>
            			<potionData type="INSTANT_HEAL" />
            		</potionMeta>
            	</icon>
            	<metadata key="guiName" value="medyk_polowy_potki" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="medic.healer.potion:1" />
            	<metadata key="item2" value="medic.healer.potion:2" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="5,2">
            	<icon id="WOOD" name="@medyk.polowy.budulecfolder.name$color" lore="@medyk.polowy.budulecfolder.lore$color,count,max" />
            	<metadata key="guiName" value="medyk_polowy_budulec" />
            	<metadata key="folder" value="true" />
            	<metadata key="item3" value="medic.healer.budulec:1" />
            	<metadata key="item3" value="medic.healer.budulec:2" />
            	<metadata key="item3" value="medic.healer.budulec:3" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="6,4">
            	<icon id="minecraft:iron_door" name="@main.return" />
            	<metadata key="guiName" value="medyk" />
            	<onClick>open</onClick>
            </button>
        </content>
    </container>
</gui>