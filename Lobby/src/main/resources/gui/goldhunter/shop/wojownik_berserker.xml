<?xml version="1.0" encoding="UTF-8" ?>
<gui height="5" title="@main.window_title">
	<container pos="0,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
	<container pos="8,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
    <container pos="1,0" size="7,5">
        <content>

            <button pos="3,0">
            	<icon id="STONE_SWORD" name="@main.wojownik.berserker.name$color" />
            </button>
            
            <button pos="1,2">
            	<icon id="WATCH" name="@wojownik.berserker.timefolder.name$color" lore="@wojownik.berserker.timefolder.lore$color,count,max"/>
            	<metadata key="guiName" value="wojownik_berserker_czas" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="warrior.berserker.time:1" />
            	<metadata key="item2" value="warrior.berserker.time:2" />
            	<metadata key="item3" value="warrior.berserker.time:3" />
            	<metadata key="item4" value="warrior.berserker.time:4" />
            	<metadata key="item5" value="warrior.berserker.time:5" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="2,2">
            	<icon id="IRON_CHESTPLATE" name="@wojownik.berserker.armor1.name$color" lore="@wojownik.berserker.armor1.lore$color,status,price"/>
            	<metadata key="item" value="warrior.berserker.ironchest" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="4,2">
            	<icon id="IRON_SWORD" name="@wojownik.berserker.miecz1.name$color" lore="@wojownik.berserker.miecz1.lore$color,status,price"/>
            	<metadata key="item" value="warrior.berserker.ironsword" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="5,2">
            	<icon id="WOOD" name="@wojownik.berserker.budulecfolder.name$color" lore="@wojownik.berserker.budulecfolder.lore$color,count,max" />
            	<metadata key="guiName" value="wojownik_berserker_budulec" />
            	<metadata key="folder" value="true" />
            	<metadata key="item3" value="warrior.berserker.budulec:1" />
            	<metadata key="item3" value="warrior.berserker.budulec:2" />
            	<metadata key="item3" value="warrior.berserker.budulec:3" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="6,4">
            	<icon id="minecraft:iron_door" name="@main.return" />
            	<metadata key="guiName" value="wojownik" />
            	<onClick>open</onClick>
            </button>
        </content>
    </container>
</gui>