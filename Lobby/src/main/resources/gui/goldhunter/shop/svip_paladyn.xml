<?xml version="1.0" encoding="UTF-8" ?>
<gui height="5" title="@main.window_title">
	<container pos="0,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
	<container pos="8,0" size="1,5">
		<background id="STAINED_GLASS_PANE" data="14" />
	</container>
    <container pos="1,0" size="7,5">
        <content>

            <button pos="3,0">
            	<icon id="GOLDEN_APPLE" name="@main.svip.paladyn.name" />
            </button>
            
            <button pos="1,2">
            	<icon id="GOLDEN_APPLE" name="@buy.svip.paladyn.name$color" lore="@buy.svip.paladyn.lore$color,status,price" />
            	<metadata key="item" value="class.svip.paladin" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="3,2">
            	<icon id="WATCH" name="@svip.paladyn.timefolder.name$color" lore="@svip.paladyn.timefolder.lore$color,count,max"/>
            	<metadata key="guiName" value="svip_paladyn_czas" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="svip.paladin.time:1" />
            	<metadata key="item2" value="svip.paladin.time:2" />
            	<metadata key="item3" value="svip.paladin.time:3" />
            	<onClick>open</onClick>
            </button>
    
            <button pos="4,2">
            	<icon id="DIAMOND_LEGGINGS" name="@svip.paladyn.armor1.name$color" lore="@svip.paladyn.armor1.lore$color,status,price"/>
            	<metadata key="item" value="svip.paladin.diamondleggins" />
            	<onClick>buy</onClick>
            </button>
            
            <button pos="5,2">
            	<icon id="WOOD" name="@svip.paladyn.budulecfolder.name$color" lore="@svip.paladyn.budulecfolder.lore$color,count,max" />
            	<metadata key="guiName" value="svip_paladyn_budulec" />
            	<metadata key="folder" value="true" />
            	<metadata key="item1" value="svip.paladin.budulec:1" />
            	<metadata key="item2" value="svip.paladin.budulec:2" />
            	<metadata key="item3" value="svip.paladin.budulec:3" />
            	<onClick>open</onClick>
            </button>
            
            <button pos="6,4">
            	<icon id="minecraft:iron_door" name="@main.return" />
            	<metadata key="guiName" value="svip" />
            	<onClick>open</onClick>
            </button>
        </content>
    </container>
</gui>