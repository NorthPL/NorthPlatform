            <!-- hearts -->
            <button pos="2,3">
                <icon id="APPLE" name="@general.name_placeholder$nameColor-hearts,name-hearts" lore="@elimination_effect.hearts$lore-hearts" />
                <onClick>northplatform://lobby/shop/general/click/bedwars_elimination/hearts/$playerId</onClick>
            </button>

            <!-- flag -->
            <button pos="3,3">
                <icon id="BANNER" name="@general.name_placeholder$nameColor-flag,name-flag" lore="@elimination_effect.flag$lore-flag" />
                <onClick>northplatform://lobby/shop/general/click/bedwars_elimination/flag/$playerId</onClick>
            </button>

            <!-- bat -->
            <button pos="4,3">
                <icon id="BANNER" name="@general.name_placeholder$nameColor-bat,name-bat" lore="@elimination_effect.bat$lore-bat" />
