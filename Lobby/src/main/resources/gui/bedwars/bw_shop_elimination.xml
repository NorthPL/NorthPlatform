                <icon id="DIAMOND" name="@general.name_placeholder$nameColor-diamonds,name-diamonds" lore="@elimination_effect.diamonds$lore-diamonds" />
                <onClick>northplatform://lobby/shop/general/click/bedwars_elimination/diamonds/$playerId</onClick>
            </button>

            <!-- grave -->
            <button pos="6,3">
                <icon id="STONE" name="@general.name_placeholder$nameColor-grave,name-grave" lore="@elimination_effect.grave$lore-grave" />
                <onClick>northplatform://lobby/shop/general/click/bedwars_elimination/grave/$playerId</onClick>
            </button>

            <!-- domyslny -->
            <button pos="4,4">
                <icon id="BARRIER" name="@general.default" />
                <onClick>northplatform://lobby/shop/general/default/bedwars_elimination/$playerId</onClick>
            </button>

            <!-- przycisk back -->
            <button pos="7,4">
                <icon id="IRON_DOOR" name="@general.back" />
                <onClick>northplatform://lobby/shop/bedwars/openCategory/main/$playerId</onClick>
