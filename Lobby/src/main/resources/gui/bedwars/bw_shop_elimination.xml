<?xml version="1.0" encoding="UTF-8" ?>
<gui height="5" title="@elimination_effect.window_title">
    <container pos="0,0" size="9,5">
        <border id="0" data="0" />
        <background id="0" data="0" />
        <content>
            <button pos="4,0">
                <icon id="IRON_SWORD" name="@elimination_effect.category_name"/>
            </button>

            <!-- firework -->
            <button pos="2,2">
                <icon id="FIREWORK" name="@general.name_placeholder$nameColor-firework,name-firework" lore="@elimination_effect.firework$lore-firework" />
                <onClick>northplatform://lobby/shop/general/click/bedwars_elimination/firework/$playerId</onClick>
            </button>

            <!-- lighting -->
            <button pos="3,2">
                <icon id="BEACON" name="@general.name_placeholder$nameColor-lighting,name-lighting" lore="@elimination_effect.lighting$lore-lighting" />
                <onClick>northplatform://lobby/shop/general/click/bedwars_elimination/lighting/$playerId</onClick>
            </button>

            <!-- boom -->
            <button pos="4,2">
                <icon id="TNT" name="@general.name_placeholder$nameColor-boom,name-boom" lore="@elimination_effect.boom$lore-boom" />
                <onClick>northplatform://lobby/shop/general/click/bedwars_elimination/boom/$playerId</onClick>
            </button>

            <!-- cookies -->
            <button pos="5,2">
                <icon id="COOKIE" name="@general.name_placeholder$nameColor-cookies,name-cookies" lore="@elimination_effect.cookies$lore-cookies" />
                <onClick>northplatform://lobby/shop/general/click/bedwars_elimination/cookies/$playerId</onClick>
